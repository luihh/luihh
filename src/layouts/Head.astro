---
const { title, description, favicon } = Astro.props;
---

<head>
  <meta charset="UTF-8" />
  <meta
    name="viewport"
    content="width=device-width, initial-scale=1.0"
  />
  <meta
    name="description"
    content={description}
  />
  <link
    rel="icon"
    href={favicon}
  />
  <meta
    name="generator"
    content={Astro.generator}
  />
  <title>{title}</title>
</head>
